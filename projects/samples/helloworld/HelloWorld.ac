namespace hello
{
	class Lol
	{
		string name = "default_name";

		void foo()
		{
			print(getMessage());
		}

		string getMessage() {
			return "My name is " + name;
		}
	}

	class Lol2 : Lol
	{
		string getMessage()
		{
			return "I'm a lol 2 named " + name + " (base: " + Lol::getMessage() + " )";
		}
	}

	// This function is called when the module has just been compiled.
	void __compiled__()
	{
		print("Hello compiled!");
