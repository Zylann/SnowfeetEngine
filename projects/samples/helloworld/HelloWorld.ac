	void onCreate()
	{
		print("onCreate was called");

		Lol a;
		Lol b = a;
		a.name = "toto";
		a.foo();
		b.foo();

		Lol@ c = a;
		a.name = "gouzi";
		c.foo();

		// auto is supported in AS 2.29.1
		// auto d = a;
		// a.name = "gouzi2";
		// d.foo();

		Lol e = Lol2();
		e.foo();

		Lol@ f = Lol2();
		f.foo();

		string[] dwarves = {"sleepy", "dobby", "sneezy", "grumpy"};
		for(uint i = 0; i < dwarves.length; ++i)
		{
			print("Dwarf " + dwarves[i]);
		}

		uint test = console::RED;
		print("red: " + test);

		unordered_map<Vector2i,string> spacehash;
		spacehash.insert(Vector2i(0,0), "origin");
		spacehash.insert(Vector2i(5,0), "right");
		spacehash.insert(Vector2i(-42,7), "far away");

		for(auto it = spacehash.begin(); ++it;)
		{
			Vector2i k = it.key;
			print("(" + k.x + "," + k.y + ") => " + it.value);
		}

		print("(5,0) => " + spacehash.find(Vector2i(-42,7)));

	}

